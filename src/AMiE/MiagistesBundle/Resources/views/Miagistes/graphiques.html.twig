{% extends "AMiEHomeBundle::layout.html.twig" %}

{% block title "Graphiques" %}

    {% block head "Statistiques sur les miagistes ayant rempli le formulaire" %}
        {% block js %}
            <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"
                    type="text/javascript"></script>

            <script src="//code.highcharts.com/4.0.1/highcharts.js"></script>
            <script src="//code.highcharts.com/4.0.1/modules/exporting.js"></script>
            <script type="text/javascript">
                {{ chart(graphContrat) }}
                {{ chart(graphEmbauche) }}
                {{ chart(graphEntAccEmb) }}
                {{ chart(graphSalaire) }}
            </script>
        {% endblock %}


        {% block content %}

            <div id="piechartContrat" style="width: 494px; height: 400px; margin: 10px 6px; float: left;"></div>
            <div id="piechartEmbauche" style="width: 494px; height: 400px; margin: 10px 6px; float: left"></div>
            <div id="barchartEntAccEmb" style="width: 1000px; height: 400px; margin: 10px 6px; float: left"></div>
            <div id="splineSalaire" style="width: 1000px; height: 400px; margin: 10px 6px; float: left"></div>


        {% endblock %}